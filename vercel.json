{
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "views",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "src/server.mjs": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.mjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 